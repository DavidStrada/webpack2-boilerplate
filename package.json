{
  "name": "es2015-sass-webpack2-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate for ES2015 and SASS using Webpack2. NOTE: 'webpack-validator' and 'webpack-bundle-size-analyzer' is not compatible with webpack-2.1.0-beta.25",
  "author": "Leif Olsen <leif.o.olsen.at.gmail.com> (https://github.com/leifoolsen)",
  "main": "webpack.config.babel.js",
  "scripts": {
    "setup": "npm install && npm-run-all clean validate",
    "validate": "npm-run-all --parallel test:coverage --serial check-coverage",
    "validate-pre-beta.25": "npm-run-all --parallel validate-webpack:* test:coverage --serial check-coverage",
    "validate-webpack:dev": "webpack-validator webpack/webpack.config.babel.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack/webpack.config.babel.js --env.prod",
    "clean": "rimraf dist",
    "prebuild": "npm-run-all clean validate",
    "build": "webpack --config webpack/webpack.config.babel.js --env.dev --progress",
    "prebuild:prod": "npm-run-all clean validate",
    "build:prod": "webpack --config webpack/webpack.config.babel.js --env.prod  --progress",
    "lint:stylelint": "stylelint './src/**/*.s?(a|c)ss' --syntax scss",
    "lint:eslint": "eslint ./src",
    "lint": "npm-run-all lint:*",
    "test:coverage": "BABEL_ENV=test babel-node ./node_modules/.bin/istanbul cover --root ./src _mocha --env.test -- --opts ./mocha.opts  './test/**/*.spec.js'",
    "check-coverage": "BABEL_ENV=test istanbul check-coverage --statements 50 --branches 50 --functions 50 --lines 50",
    "test": "BABEL_ENV=test mocha --env.test --opts ./mocha.opts './test/**/*.spec.js'",
    "test:watch": "npm run test -- --watch",
    "test:pattern": "npm run test -- --watch --grep $pattern",
    "analyze": "webpack --config webpack/webpack.config.babel.js --env.prod --json | webpack-bundle-size-analyzer",
    "start:build": "babel-node webpack/server --env.prod --port 8000 --presets latest,stage-0",
    "start": "babel-node webpack/server --env.dev --port 8084 --presets latest,stage-0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "node_modules/.bin/opt --in pre-commit --exec \"npm run validate\""
    }
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leifoolsen/webpack2-boilerplate.git"
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "moment": "2.16.0",
    "whatwg-fetch": "2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "6.5.3",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.8",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-plugin-istanbul": "3.0.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "body-parser": "1.15.2",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.0",
    "eslint": "3.10.2",
    "eslint-loader": "1.6.1",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "ghooks": "1.3.2",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "ignore-styles": "5.0.1",
    "imports-loader": "0.6.5",
    "ip": "1.1.4",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.8.3",
    "json-loader": "0.5.4",
    "material-design-icons": "3.0.1",
    "mocha": "3.1.2",
    "node-sass": "3.13.0",
    "npm-run-all": "3.1.1",
    "offline-plugin": "3.4.2",
    "opt-cli": "1.5.1",
    "postcss-loader": "1.1.1",
    "precss": "^1.4.0",
    "resolve-url-loader": "1.6.0",
    "rimraf": "2.5.4",
    "roboto-fontface": "0.6.0",
    "sass-loader": "4.0.2",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "stylelint": "7.6.0",
    "stylelint-config-standard": "15.0.0",
    "stylelint-webpack-plugin": "0.4.0",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-bundle-size-analyzer": "2.2.0",
    "webpack-dev-middleware": "1.8.4",
    "webpack-dev-server": "2.1.0-beta.9",
    "webpack-hot-middleware": "2.13.2",
    "webpack-validator": "2.2.9"
  },
  "keywords": [
    "Webpack2",
    "Webpack",
    "JavaScript",
    "ES6",
    "ES2015",
    "ECMASCRIPT 2015",
    "Babel",
    "SASS",
    "SASS Boilerplate",
    "Mocha",
    "Chai",
    "Istanbul"
  ],
  "license": "MIT"
}
